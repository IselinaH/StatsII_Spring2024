library(tidyverse)
library(stargazer)
library(readr)
dublinbikes_20210101_20210401 <- read_csv("~/Desktop/dublinbikes_20210101_20210401.csv")
View(dublinbikes_20210101_20210401)
# Define a range of values for t
t <- seq(0, 2*pi, length.out = 100)
# Define the parametric equations for a heart shape
x <- 16 * sin(t)^3
y <- 13 * cos(t) - 5 * cos(2*t) - 2 * cos(3*t) - cos(4*t)
# Plot the heart shape
plot(x, y, type = "l", col = "red", xlab = "", ylab = "", main = "Heart Shape", asp = 1)
# Print the plot
print(plot(x, y, type = "l", col = "red", xlab = "", ylab = "", main = "Heart Shape", asp = 1))
